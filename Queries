CREATE TABLE `clients` (
    `ID` INT PRIMARY KEY AUTO_INCREMENT,
    `Name` VARCHAR(100),
    `Surname` VARCHAR(100),
    `Username` VARCHAR(100) UNIQUE,
    `user_password` CHAR(64),
    `Email` VARCHAR(255) UNIQUE,
    `Age` INT,
    `Registration_Date` DATE
);

