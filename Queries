CREATE TABLE packages (
    ID INT PRIMARY KEY,
    Name VARCHAR(255),
    Type VARCHAR(255),
    Price DECIMAL(10,2),
    Date DATE,
    endDate DATE,
    Place VARCHAR(255),
    Description TEXT,
    available_spots INT,
    image_path VARCHAR(255),
    Duration INT
);

