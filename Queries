CREATE TABLE employees (
    ID INT PRIMARY KEY,
    profile_image_path VARCHAR(255),
    Name VARCHAR(255),
    Surname VARCHAR(255),
    Username VARCHAR(255),
    user_password VARCHAR(255),
    Email VARCHAR(255),
    Age INT,
    Registration_Date DATE,
    Role VARCHAR(255),
    addedBy INT 
    FOREIGN KEY (`addedBy`) REFERENCES `employees`(`ID`)
);

